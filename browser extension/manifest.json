{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "permissions": ["storage", "activeTab", "scripting"],
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "popup.js",
    "type": "module"
  }
}